<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd ">

    <dubbo:provider filter="exceptionInterceptor"/>
    <!--ID发号服务-->
    <dubbo:service interface="com.yryz.basic.modules.id.api.IdAPI" ref="idAPI"
                   registry="sgt"/>
    <!--发号配置服务，系统配置服务，业务服务不得引用-->
    <dubbo:service interface="com.yryz.basic.modules.id.api.CodeAPI" ref="codeAPI"
                   registry="sgt"/>


</beans>